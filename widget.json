{
  "name": "Photo Sharing",
  "package": "com.fliplet.photo-sharing",
  "version": "1.0.0",
  "icon": "img/icon.png",
  "tags": ["category:feeds"],
  "provider_only": false,
  "html_tag": "span",
  "interface": {
    "dependencies": [
      "fliplet-core",
      "fliplet-datasources",
      "fliplet-media",
      "fliplet-studio-ui",
      "handlebars",
      "bootstrap",
      "lodash"
    ],
    "assets": [
      "js/interface.js",
      "css/interface.css"
    ]
  },
  "build": {
    "dependencies": [
      "bootstrap",
      "fliplet-core",
      "fliplet-security",
      "fliplet-datasources",
      "handlebars",
      "font-awesome",
      "animate-css",
      "moment"
    ],
    "assets": [
      "vendor/overlay.js",
      "vendor/imageUpload.js",
      "js/build.js",
      "vendor/overlay.css",
      "css/build.css",
      "img/image-placeholder.png",
      "img/image-placeholder-2.png"
    ]
  },
  "references": [
    "dataSourceId:dataSource",
    "data.folder.folderId:mediaFolder",
    "data.folder.appId:app",
    "data.folder.organizationId:organization"
  ]
}
